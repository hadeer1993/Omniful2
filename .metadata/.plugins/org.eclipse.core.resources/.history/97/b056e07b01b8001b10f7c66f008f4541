package selenium.Omniful;

import org.testng.annotations.Test;
import static io.restassured.RestAssured.*;

public class myFirstRestAssuredClass {
	@Test(priority=1)
	public static void getResponseBody(){
		given()
		.queryParam("transactions ","{\"pos_id\" : 7,\r\n" + 
				"            \"sku\" : \"Fight Like a Girl Tote - Large-Dusty Pink\",\r\n" + 
				"            \"transaction_date\" : 1615904223,\r\n" + 
				"            \"transaction_id\" : 3000,\r\n" + 
				"            \"quantity\" : -5,\r\n" + 
				"            \"return_pos_id\" : 7,\r\n" + 
				"            \"terminal_id\" : 119,\r\n" + 
				"            \"net_amount\" : 500,\r\n" + 
				"            \"vat\" : 10,\r\n" + 
				"            \"tax_amount\" : 50,\r\n" + 
				"            \"price\" : 20,\r\n" + 
				"            \"return_transaction_id\" : 2}")
		.when()
		.post("https://osama.dashboard-omniful-stage.ibtikar.sa/api/v1/client/pos")
		.then()
		.log()
		.all();
	}
}
